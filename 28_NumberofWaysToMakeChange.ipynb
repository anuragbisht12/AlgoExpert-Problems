        "# O(n of denoms) time | O(n) for space\n",
        "def numberOfWaysToMakeChange(n, denoms):\n",
        "\tways=[0 for amount in range(n+1)]\n",
        "\tways[0]=1\n",
        "\tfor denom in denoms:\n",
        "\t\tfor amount in range(1,n+1):\n",
        "\t\t\tif denom<=amount:\n",
        "\t\t\t\tways[amount]+=ways[amount-denom]\n",
        "\treturn ways[n]"
      ],
      "execution_count": 0,
