      "source": [
        "#O(n) time | O(1) space\n",
        "def kadanesAlgorithm(array):\n",
        "    maxEndingHere=array[0]\n",
        "\tmaxSoFar=array[0]\n",
        "\tfor i in range(1,len(array)):\n",
        "\t\tnum = array[i]\n",
        "\t\tmaxEndingHere =max(num,maxEndingHere+num)\n",
        "\t\tmaxSoFar= max(maxSoFar,maxEndingHere)\n",
        "\treturn maxSoFar\n"
      ],
      "execution_count": 0,
      "outputs": []
    }
  ]
}