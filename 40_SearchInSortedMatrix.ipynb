      "source": [
        "#O(n+m) time | O(1)\n",
        "def searchInSortedMatrix(matrix, target):\n",
        "    row=0\n",
        "\tcol=len(matrix[0])-1\n",
        "\twhile row < len(matrix) and col>=0:\n",
        "\t\tif matrix[row][col] > target:\n",
        "\t\t\tcol -=1\n",
        "\t\telif matrix[row][col] < target:\n",
        "\t\t\trow+=1\n",
        "\t\telse:\n",
        "\t\t\treturn [row,col]\n",
        "\treturn [-1,-1]\n",
        "\t\n"
      ],
      "execution_count": 0,
      "outputs": []
    }
  ]
